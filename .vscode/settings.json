{
  "scss.lint.unknownAtRules": "ignore",
  "postcss.validate": false,
  "css.validate": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.rulers": [100],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.eol": "\n",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#fd8955",
    "activityBar.background": "#fd8955",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#c0fed3",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#fd8955",
    "statusBar.background": "#fc6623",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#e94a03",
    "statusBarItem.remoteBackground": "#fc6623",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#fc6623",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#fc662399",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#FC6623"
}
